<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace需要和Mapper接口对应-->
<mapper namespace="com.cykj.admin.mapper.AddRedisMapper">
    <insert id="insertTestData" parameterType="com.cykj.pojo.Fee" >
        insert into tb_fee(bus_id, user_id, site_id, line_id)
        values(#{busId}, #{userId}, #{siteId}, #{lineId})
    </insert>

</mapper>